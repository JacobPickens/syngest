{
  "name": "scan-dashboard-pug",
  "version": "1.3.0",
  "private": true,
  "description": "Non-interfering Express + Pug scan dashboard that polls SQLite and a log file.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dump:generate": "node ./version-control/generate-dump.js",
    "dump:apply": "node ./version-control/dump-apply.js",
    "dump:active": "node ./version-control/print-active.js",
    "dump:undo": "node ./version-control/dump-undo.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "git": "^0.1.5",
    "morgan": "^1.10.0",
    "pug": "^3.0.3",
    "sqlite3": "^5.1.7",
    "xxhash-wasm": "^1.1.0"
  }
}
